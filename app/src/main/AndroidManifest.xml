<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projetos.lucaschr.projeto01">

    <meta-data android:name="com.google.android.geo.API_KEY" android:value="your API key"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".principal.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".principal.PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".denuncia.DenunciaCadActivity" />
        <activity android:name=".denuncia.DenunciaActivity"></activity>
    </application>

</manifest>